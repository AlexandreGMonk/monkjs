{
  "expo": {
    "name": "Monk",
    "slug": "Monk",
    "owner": "monkvision",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#274b9f",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#274b9f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.monkvision.monk",
      "buildNumber": "0.0.1",
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "package": "com.monkvision.monk",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#274b9f"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
